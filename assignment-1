#include<stdio.h>
int main()
{
    int r,c,fstmat[10][10],secmat[10][10],sum[10][10],i,j;
    printf("\n enter the number of rows and colums:");
    scanf("%d %d",&r,&c);
    printf("\n enterr the first matrix element;");
    for(i=0;i<r;++i)
    for(j=0;j<c;++j)
    { 
        scanf("%d",&fstmat[i][j]);
    }
    printf("\n 1st matrix \n");
    for(i=0;i<r;i++)
    {
    for(j=0;j<c;j++)
    { 
    printf("%d",fstmat[i][j]);
    }
        printf("\n");
    }
    printf("\n enter 2nd matrix elements:");
    for(i=0;i<r;++i)
    for(j=0;j<c;++j)
    {
    scanf("%d",&secmat[i][j]);
    }
    printf("\n 2nd matrix \n");
    for(i=0;i<r;i++)
    {
    for(j=0;j<c;j++)
    {
    printf("%d",secmat[i][j]);
    }
    printf("\n");
    }
    printf("\n added af two matrix\n");
    for(i=0;i<r;++i)
    for(j=0;j<c;++j)
    {
    sum[i][j]=fstmat[i][j]+secmat[i][j];
    }
    printf("\n subtract of two matrix \n");
    {
    sum[i][j]=fstmat[i][j]-secmat[i][j];
    }
    for(i=0;i<r;i--)
    for(j=0;j<c;j--)
    {
    printf("%d\n",sum[i][j]);

    {
    printf("\n");
    }return 0;

    }
    }
